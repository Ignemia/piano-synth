{
  "audio": {
    "sample_rate": 44100.0,
    "buffer_size": 512,
    "channels": 2,
    "output_device": "default",
    "enable_exclusive_mode": false,
    "latency_mode": "low"
  },
  "string": {
    "tension_base": 1000.0,
    "damping": 0.001,
    "stiffness": 1e-5,
    "density": 7850.0,
    "discretization_points": 100,
    "coupling_strength": 0.1
  },
  "hammer": {
    "mass": 0.01,
    "stiffness": 1e6,
    "damping": 100.0,
    "contact_time": 0.001,
    "felt_hardness": 0.5,
    "velocity_scaling": 1.0
  },
  "soundboard": {
    "area": 0.5,
    "thickness": 0.01,
    "density": 400.0,
    "damping": 0.01,
    "resonance_strength": 0.7,
    "coupling_factor": 0.3
  },
  "resonance": {
    "max_harmonics": 32,
    "harmonic_decay": 0.8,
    "sympathetic_resonance": 0.1,
    "string_coupling": true,
    "soundboard_coupling": true
  },
  "synthesis": {
    "max_voices": 128,
    "note_off_fade_time": 0.1,
    "velocity_sensitivity": 0.01,
    "master_volume": 0.8,
    "polyphony_mode": "unlimited",
    "voice_stealing": "oldest"
  },
  "midi": {
    "device_name": "",
    "auto_detect": true,
    "velocity_curve": 1.0,
    "hammer_response_curve": 1.0,
    "channel_filter": -1,
    "transpose": 0,
    "enable_aftertouch": true,
    "enable_pitch_bend": true
  },
  "pedals": {
    "sustain_threshold": 64,
    "soft_threshold": 64,
    "sostenuto_threshold": 64,
    "sustain_curve": 1.0,
    "soft_effect_strength": 0.3,
    "sostenuto_mode": "realistic"
  },
  "recording": {
    "mp3_bitrate": 192,
    "mp3_quality": 5,
    "output_directory": "./recordings/",
    "auto_filename": true,
    "filename_format": "recording_%Y%m%d_%H%M%S.mp3",
    "enable_normalization": true,
    "normalization_level": -3.0
  },
  "room": {
    "size": 10.0,
    "reverb_time": 1.5,
    "damping": 0.3,
    "early_reflections": true,
    "late_reverb": true,
    "room_type": "concert_hall"
  },
  "tuning": {
    "reference_frequency": 440.0,
    "temperament": "equal",
    "master_tuning": 0.0,
    "stretch_tuning": 0.0,
    "individual_note_tuning": {}
  },
  "performance": {
    "audio_thread_priority": "high",
    "midi_thread_priority": "normal",
    "buffer_prefill": 2,
    "cpu_optimization": "auto",
    "memory_pool_size": 64,
    "enable_multithreading": true
  },
  "user_interface": {
    "enable_cli": true,
    "log_level": "info",
    "log_to_file": false,
    "log_filename": "piano_synth.log",
    "show_performance_stats": false,
    "enable_hot_reload": true
  },
  "advanced": {
    "string_nonlinearity": 0.1,
    "hammer_felt_compression": true,
    "soundboard_vibration_modes": 12,
    "air_absorption": 0.001,
    "temperature_compensation": 20.0,
    "humidity_compensation": 50.0,
    "enable_experimental_features": false
  }
}