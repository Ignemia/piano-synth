{
  "abstraction_processor": {
    "version": "1.0.0",
    "velocity_processing": {
      "velocity_curve": 1.0,
      "velocity_sensitivity": 1.0,
      "min_velocity": 0.01,
      "max_velocity": 1.0,
      "velocity_smoothing": 0.1
    },
    "timing": {
      "enable_quantization": false,
      "quantization_strength": 0.0,
      "timing_correction": 0.0,
      "note_off_delay_compensation": 0,
      "max_note_duration_ms": 30000
    },
    "pedal_processing": {
      "sustain_sensitivity": 1.0,
      "soft_pedal_enabled": true,
      "sostenuto_enabled": true,
      "pedal_smoothing": 0.05,
      "sustain_threshold": 0.5
    },
    "expression": {
      "pitch_bend_range_semitones": 2.0,
      "aftertouch_sensitivity": 1.0,
      "modulation_depth": 1.0,
      "breath_controller_enabled": false
    },
    "note_management": {
      "enable_note_stealing": true,
      "max_active_notes": 256,
      "note_priority": "velocity",
      "overlapping_note_behavior": "retrigger"
    },
    "filtering": {
      "enable_duplicate_filtering": true,
      "velocity_threshold": 1,
      "timing_threshold_ms": 1
    }
  }
}
